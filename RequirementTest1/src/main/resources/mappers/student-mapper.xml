<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.kh.test.model.mapper.StudentMapper">

	<insert id="addStudent">
		INSERT INTO STUDENT
		VALUES(SEQ_STUDENT_NO.NEXTVAL, #{studentName}, #{studentMajor}, #{studentGender})
	</insert>

</mapper>

